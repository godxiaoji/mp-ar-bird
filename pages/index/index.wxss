/* pages/posnet/index.wxss */
page {
  display: flex;
  flex-direction: column;
  background: #00a300
    url('data:image/png;base64,')
    repeat-x 0 133.32vw;
  height: 100%;
}

.page-head {
  height: 133.3333vw;
  background-color: #008793;
}

.camera {
  width: 100vw;
  height: 133.3333vw;
  z-index: 5;
}

.canvas {
  width: 100%;
  height: 100%;
  z-index: 10;
}

.result {
  position: absolute;
  top: 50%;
  left: 50%;
}

.prediction-result {
  height: calc(100vh - 100vw);
}

.page-body {
  padding: 30px 0 0 0;
  display: flex;
  justify-content: center;
  align-items: center;
}

.page-body {
  font-size: 16px;
  color: #fff;
}

.deny-tips {
  display: flex;
  flex-direction: column;
  align-items: center;
  font-size: 16px;
  color: #fff;
  line-height: 32px;
}
